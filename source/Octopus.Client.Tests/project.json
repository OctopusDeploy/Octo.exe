{
  "version": "0.0.0-*",
  "dependencies": {
    "Serilog": "2.1.0",
    "Octopus.Client": {
      "target": "project"
    },
    "NUnit": "3.4.1",
    "dotnet-test-nunit": "3.4.0-beta-2",
    "NSubstitute": "2.0.0-rc",
    "FluentAssertions": "4.12.0",
    "Autofac": "4.1.0",
    "Nancy": "2.0.0-barneyrubble",
